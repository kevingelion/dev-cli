{
  "name": "notarize",
  "description": "CLI to help setup and run various Notarize development services",
  "version": "0.0.1",
  "author": "Kevin Murphy",
  "bin": {
    "notarize-dev": "./bin/run"
  },
  "bugs": "https://github.com/kevingelion/notarize/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "execa": "^5.0.0",
    "listr": "^0.14.3",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/listr": "^0.14.2",
    "@types/node": "^14.14.35",
    "eslint": "^7.22.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.2",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.2.8",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kevingelion/notarize",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "notarize-dev",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kevingelion/notarize",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "tsc -b && tsc-alias -p tsconfig.json && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  }
}
